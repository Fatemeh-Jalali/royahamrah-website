					<span class="login100-form-title" style="padding: 16px 0;">
						<%= title %>
					</span>
                    <div class="alert-box">
                        <%= message %>
                    </div>
	
					<div class="container-login100-form-btn">
						<a href="/" class="login100-form-btn" style="border-radius: 36px; text-decoration: none;">صفحه اصلی سایت</a>
					</div>
				</div>
			</div>
		</div>
	</div>

